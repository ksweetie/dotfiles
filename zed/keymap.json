[
  {
    "bindings": {
      "cmd-shift-r": ["workspace::SendKeystrokes", "cmd-shift-p workspace: reload enter"],
      "cmd-shift-e": "project_panel::ToggleFocus",
      "cmd-shift-g": "git_panel::ToggleFocus",
      "ctrl-alt-cmd-space": "project_panel::CollapseAllEntries",
      "alt-shift-w": "workspace::ClearAllNotifications",
      "shift-escape": null // Unbind default binding
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-shift-j": ["task::Spawn", { "task_name": "run spec" }],
      "cmd-shift-e": "pane::RevealInProjectPanel",
      "cmd-shift-c": ["workspace::SendKeystrokes", "cmd-shift-e ctrl-alt-cmd-space cmd-shift-e"],
      "cmd-alt-shift-c": ["workspace::SendKeystrokes", "cmd-shift-p editor: copy file location enter"],
      "cmd-y": "editor::DuplicateLineDown",
      "alt-{": "pane::SwapItemLeft",
      "alt-}": "pane::SwapItemRight"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-shift-c": "project_panel::CollapseAllEntries"
    }
  },
  {
    "context": "Editor && edit_prediction",
    "bindings": {
      "tab": null // Unbind default binding
    }
  }
]
